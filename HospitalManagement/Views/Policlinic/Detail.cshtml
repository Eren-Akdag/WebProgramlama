@model Policlinic
<h2>Polikinlik Detay Sayfasi</h2>

<form method="post">



	<div class="border p-3 mt-5">

		<div class="row pb-3  form-group">
			<h2 class="text-primary "><strong>Polikinlik Detay</strong> </h2>
			</hr>
		</div>


		<div class="mb-3  form-group"
		<label asp-for="Name"></label>
		<input asp-for="Name" disabled class="form-control">

		</div>

		<div class="mb-3  form-group"
		<label asp-for=Address></label>
		<input asp-for="Address" disabled class="form-control">

		</div>

		<br />
		

		<a asp-controller="Policlinic" asp-action="Index" class="btn btn-outline-info style="width:250px">
			Polikinlik Listesine Dön
		</a>

	</div>






</form>